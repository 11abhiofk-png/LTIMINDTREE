{
  "_version": "1.76.0",
  "sap.app": {
    "id": "project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.1",
      "toolsId": "c2f61aef-0483-446a-ab06-b0da47dc303e"
    },

"dataSources": {
  "Northwind2": {
    "uri": "http://localhost:8081/https://services.odata.org/v3/northwind2/northwind2.svc/",
    "type": "OData",
    "settings": {
      "odataVersion": "3.0",
      "localUri": "localService/metadata.xml"
    }
  }
}

  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.141.3",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },


"models": {
  "i18n": {
    "type": "sap.ui.model.resource.ResourceModel",
    "settings": {
      "bundleName": "project1.i18n.i18n"
    }
  },
  "": {
    "type": "sap.ui.model.odata.v2.ODataModel",
    "dataSource": "Northwind2",
    "settings": {
      "defaultBindingMode": "TwoWay",
      "tokenHandling":false
    }
  }
},


    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "project1.view",
        "async": true,
        "viewPath": "project1.view",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteSap1",
          "pattern": ":?query:",
          "target": [
            "TargetSap1"
          ]
        },
        {
          "name": "Routedashboard",
          "pattern": "list/{name}/{pass}",
          "target": [
            "Targetdashboard"
          ]
        }
      ],
      "targets": {
        "TargetSap1": {
          "id": "Sap1",
          "name": "Sap1"
        },
        "Targetdashboard": {
          "id": "dashboard",
          "name": "dashboard"
        }
      }
    },
    "rootView": {
      "viewName": "project1.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
